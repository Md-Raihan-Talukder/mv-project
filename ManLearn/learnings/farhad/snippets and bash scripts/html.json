            "            <md-icon md-font-icon=\"icon-close\"></md-icon>",
            "        </md-button>",
            "    </md-toolbar>",
            "",
            "    <md-dialog-content class=\"m-0 p-0\" style=\"height: 100%;\">",
            "        <div class=\"b grey-200-bg\" layout=\"row\" layout-align=\"start center\" style=\"height: 7%;\">",
            "            <span class=\"secondary-text mr-5 ml-10\">Field1:</span>",
