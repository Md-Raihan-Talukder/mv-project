    "Accordion 1D": {
        "prefix": "snpt-accordion1D",
        "body": [
            "<msb-expand-collapse ng-repeat=\"obj in vm.accordion1D\">",
            "    <div class=\"msb-collapse-header blue-grey-50-bg mb-5 h-40 b cursor-pointer ml-10 mr-10\" layout=\"row\" ng-click=\"(clicked) ? clicked=false: clicked=true\" layout-align=\"space-between center\">",
            "        <div class=\"p-10 grey-700-fg font-size-16\" layout=\"row\" layout-align=\"space-between center\">",
            "            <span class=\"secondary-text mr-5\">Field1 :</span>",
            "            <span class=\"text-bold mr-40\">{{obj.field1}}</span>",
            "            <span class=\"secondary-text mr-5\">Field2 :</span>",
            "            <span class=\"text-bold mr-40\">{{obj.field2}}</span>",
            "            <span class=\"secondary-text mr-5\">Field3 :</span>",
            "            <span class=\"text-bold\">{{obj.field2}}</span>",
            "        </div>",
            "        <div layout=\"row\" ng-if=\"\\$index==0\">",
            "            <md-icon ng-if=\"clicked\" md-font-icon=\"icon-chevron-right\" class=\"s16 md-accent mr-20\">",
            "            </md-icon>",
            "            <md-icon ng-if=\"!clicked\" md-font-icon=\"icon-chevron-down\" class=\"s16 md-accent mr-20\">",
            "            </md-icon>",
            "        </div>",
            "        <div layout=\"row\" ng-if=\"\\$index!=0\">",
            "            <md-icon ng-if=\"!clicked\" md-font-icon=\"icon-chevron-right\" class=\"s16 md-accent mr-20\">",
            "            </md-icon>",
            "            <md-icon ng-if=\"clicked\" md-font-icon=\"icon-chevron-down\" class=\"s16 md-accent mr-20\">",
            "            </md-icon>",
            "        </div>",
            "    </div>",
            "    <div class=\"msb-collapse-content ml-10 mr-10\" ng-style=\"\\$index!=0 && {display:'none'}\">",
            "        <!-- collapse content here -->",
            "    </div>",
            "</msb-expand-collapse>"
        ],
        "description": "Collapse Array"
    },
