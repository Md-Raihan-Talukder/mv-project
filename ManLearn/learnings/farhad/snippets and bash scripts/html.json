    "Collapse Table": {
        "prefix": "snpt-collapseTable",
        "body": [
            "<div class=\"m-10\" style=\"width: 1000px;\">",
            "    <msb-expand-collapse ng-repeat=\"obj in vm.collapseTable\">",
            "        <div class=\"msb-collapse-header cursor-pointer h-45 blue-grey-50-bg b mb-5\" layout=\"row\" layout-align=\"space-around\">",
            "            <div class=\"font-size-16\" layout=\"column\" layout-align=\"center center\">Field1</div>",
            "            <div class=\"font-size-16\" layout=\"column\" layout-align=\"center center\">Field2</div>",
            "            <div class=\"font-size-16\" layout=\"column\" layout-align=\"center center\">Field3</div>",
            "            <div class=\"font-size-16\" layout=\"column\" layout-align=\"center center\">Field4</div>",
            "            <div class=\"font-size-16\" layout=\"column\" layout-align=\"center center\">Field5</div>",
            "            <div class=\"font-size-16\" layout=\"column\" layout-align=\"center center\">Field6</div>",
            "        </div>",
            "        <div class=\"msb-collapse-content white-bg\" ng-style=\"\\$index!=0 && {display:'none'}\">",
            "            <div ng-repeat=\"attribute in obj.childArray\" layout=\"row\" class=\"\" layout-align=\"space-around\">",
            "                <div class=\"h-50 bb br bl\" flex layout=\"column\" layout-align=\"center center\">{{attribute.field1}}</div>",
            "                <div class=\"h-50 bb br\" flex layout=\"column\" layout-align=\"center center\">{{attribute.field2}}</div>",
            "                <div class=\"h-50 bb br\" flex layout=\"column\" layout-align=\"center center\">{{attribute.field3}}</div>",
            "                <div class=\"h-50 bb br\" flex layout=\"column\" layout-align=\"center center\">{{attribute.field4}}</div>",
            "                <div class=\"h-50 bb br\" flex layout=\"column\" layout-align=\"center center\">{{attribute.field5}}</div>",
            "                <div class=\"h-50 bb br\" flex layout=\"column\" layout-align=\"center center\">{{attribute.field6}}</div>",
            "            </div>",
            "        </div>",
            "    </msb-expand-collapse>",
            "</div>"
        ],
        "description": "Collapse Table"
    },
